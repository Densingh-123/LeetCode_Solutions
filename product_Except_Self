import java.util.*;
class Main {
    public static void main(String[] args) {
        int[] arr={1,2,3,4};
        int n = arr.length;
        int[] res = new int[n];
        for(int i=0;i<n;i++){
            int pro=1;
            for(int j=0;j<n;j++){
                if(arr[i]!=arr[j]){
                    pro*=arr[j];
                }
                res[i]=pro;
            }
        }
        System.out.print(Arrays.toString(res));
    }
}
